{
  "name": "weaver-frontend-cli",
  "version": "2.1.3",
  "description": "🕷️ Weaver CLI - Generador completo de arquitectura Clean Architecture para entidades CRUD y flujos de negocio desde OpenAPI/Swagger",
  "main": "dist/index.js",
  "bin": {
    "weaver": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dandres10/weaver-cli.git"
  },
  "homepage": "https://github.com/dandres10/weaver-cli#readme",
  "bugs": {
    "url": "https://github.com/dandres10/weaver-cli/issues"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test:local": "npm run build && node dist/cli.js --local",
    "test:generate": "npm run build && node dist/cli.js",
    "logout": "npm run build && node dist/cli.js --logout",
    "session-info": "npm run build && node dist/cli.js --session-info",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "keywords": [
    "weaver",
    "cli",
    "generator",
    "frontend",
    "typescript",
