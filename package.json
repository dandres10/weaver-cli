    "code-generator",
    "entity-generator",
    "business-flow-generator",
    "swagger",
    "openapi",
    "use-cases",
    "repositories",
    "facades",
    "dto-generator",
    "entity-mapper",
    "dependency-injection",
    "crud-generator"
  ],
  "author": "Andrés León",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/inquirer": "^8.2.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "dependencies": {
    "inquirer": "^8.2.5",
    "chalk": "^4.1.2",
    "fs-extra": "^11.0.0",
    "@types/fs-extra": "^11.0.0",
    "axios": "^1.6.0",
    "@apidevtools/swagger-parser": "^10.1.0",
    "openapi-types": "^12.1.3"
